#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xming/Projects/qilv/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.2_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_a23l64uaqcwoxurw2k4nw74alm/node_modules/ts-jest/node_modules:/Users/xming/Projects/qilv/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.2_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_a23l64uaqcwoxurw2k4nw74alm/node_modules:/Users/xming/Projects/qilv/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xming/Projects/qilv/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.2_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_a23l64uaqcwoxurw2k4nw74alm/node_modules/ts-jest/node_modules:/Users/xming/Projects/qilv/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.2_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_a23l64uaqcwoxurw2k4nw74alm/node_modules:/Users/xming/Projects/qilv/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
